Hej
<div style="width:60%">
      <canvas id="myE4Chart" width="400" height="400"></canvas>
      </div>
      <script>
      var ctx = document.getElementById('myE4Chart').getContext('2d');
      var scatterChart = new Chart(ctx, {
          type: 'scatter',
          data: {
        	  datasets: [{
                  label: 'Scatter Made',
                  data: [{
                      x: 0,
                      y: 0
                  }],
                  pointBackgroundColor:"#FF0000",
                  pointBorderColor:',
                  pointRadius: 7.5,
              }]
          }
      })
      window.setInterval(addData, 5000)
      
      function addData() {
    	  console.log("imma give U sum mfing data!");
    	  var ranx = Math.floor(Math.random() * 10);
    	  var rany = Math.floor(Math.random() * 10);
    	  console.log(scatterChart.config.data.datasets[0].data);
    	  scatterChart.config.data.datasets[0].data.push(ranx, rany);
    	  scatterChart.update();
      }
    	  
      </script>

</body>
